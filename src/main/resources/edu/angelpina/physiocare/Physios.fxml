<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="edu.angelpina.physiocare.Controllers.PhysiosController">
    <ListView fx:id="physiosList" prefHeight="400.0" prefWidth="600.0"/>
    <HBox alignment="CENTER" spacing="10.0">
        <Button fx:id="btnAdd" text="Add Patient" onAction="#addPhysio"/>
        <Button text="Back to Title" onAction="#goToTitle"/>
    </HBox>
</VBox>